<ul>
<% @bookmark_list.each do |bookmark| %>
<li><a href="<%= bookmark.url %>" target="_blank"><%= bookmark.title %></a></li>
<% end %>
</ul>

<form action='/bookmarks/new'>
<input type='submit' value='Add Bookmark'>
</form>


